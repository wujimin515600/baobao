page {
  background: #FAFAFA;
}

#index .index-banner {
  margin: 0 auto;
  height: 290rpx;
  display: block;
  background: #fff;
}

#index .cateList {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 0 30rpx 30rpx;
  background: #fff;
}

#index .cateList .cList {
  display: flex;
  justify-content: space-between;
  box-shadow: 0 3rpx 23rpx 1rpx rgba(77, 77, 77, 0.1);
  border-radius: 8rpx;
  background: #fff;
  padding: 37rpx 15rpx;
}

#index .cateList .cList .icon_img {
  width: 81rpx;
  height: 81rpx;
}

#index .cateList .cList .cate_info {
  margin-left: 23rpx;
}

#index .cateList .cList .cate_info .cate_info_title {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #3A4159;
  line-height: 32rpx;
}

#index .cateList .cList .cate_info .cate_info_info {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #858EA0;
  line-height: 24rpx;
  margin-top: 14rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  width: 200rpx;
  display: flex;
  align-items: center;
}

#index .cateList .cList .cate_info .cate_info_info text {
  margin-left: 3rpx;
}

#index .cateList .cList:nth-child(n+3) {
  margin-top: 24rpx;
}

#index .nearby {
  margin-top: 30rpx;
  background: #fff;
  height: 100vw;
}

#index .nearby .nearby-title {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  background: #fff;
  padding: 15rpx 30rpx;
}

#index .nearby .nearby-title .title {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #171A25;
}

#index .nearby .nearby-title .title-href {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #858EA0;
  line-height: 45rpx;
}

#index .nearby .nearby-title .title-href .nearby-titleicon {
  width: 12rpx;
  height: 22rpx;
  margin-left: 13rpx;
}

#index .nearby .nearby-list {
  margin-top: 4rpx;
  padding: 0 30rpx 30rpx;
}

#index .nearby .twoButton {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  margin-top: 30rpx;
  padding: 0 125rpx;
}

#index .nearby .twoButton view {
  width: 217rpx;
  height: 69rpx;
  font-size: 26rpx;
  border-radius: 48rpx;
  font-family: PingFangSC-Regular;
  text-align: center;
  line-height: 69rpx;
}

#index .nearby .twoButton .refresh {
  border: 2rpx solid #2576F8;
  color: #2576F8;
}

#index .nearby .twoButton .refresh .refreshicon {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  width: 25rpx;
  height: 25rpx;
  display: inline-block;
  vertical-align: -3rpx;
  margin-right: 14rpx;
}

#index .nearby .twoButton .phone {
  background: linear-gradient(-90deg, #215FF4 0%, #298CFC 100%);
  color: #FFFFFF;
  margin-left: 65rpx;
}

#index .nearby .twoButton .phone .phoneicon {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  width: 22rpx;
  height: 27rpx;
  display: inline-block;
  vertical-align: -3rpx;
  margin-right: 14rpx;
}

#index .shouhuo {
  background: #FFFFFF;
  box-shadow: 0 3rpx 23rpx 1rpx rgba(77, 77, 77, 0.1);
  border-radius: 8rpx;
  width: 690rpx;
  height: 248rpx;
  padding: 30rpx 0;
  position: fixed;
  bottom: 0;
  left: 50%;
  margin-left: -345rpx;
  z-index: 10;
}

#index .shouhuo .title {
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #171A25;
  text-align: center;
}

#index .shouhuo .daojishi {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #2576F8;
  text-align: center;
  margin: 25rpx 0 12rpx;
}

#index .shouhuo .people {
  margin: 18rpx auto 16rpx;
  width: 473rpx;
}

#index .shouhuo .people image {
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto 10rpx;
  display: block;
}

#index .shouhuo .people text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #3A4159;
  text-align: center;
  display: block;
}

#index .shouhuo .quxiao {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #3A4159;
  border: 2rpx solid #C9CDD7;
  border-radius: 34rpx;
  text-align: center;
  width: 190rpx;
  height: 68rpx;
  line-height: 68rpx;
  margin: 0 auto;
}

#index .shouhuo .o_stateTwo {
  width: 473rpx;
  margin: 16rpx auto 0;
  display: flex;
}

#index .shouhuo .o_stateTwo .cancalButton {
  width: 217rpx;
  height: 69rpx;
  line-height: 69rpx;
  border: 2rpx solid #858EA0;
  border-radius: 48rpx;
  color: #858EA0;
  text-align: center;
  font-size: 28rpx;
}

#index .shouhuo .o_stateTwo .callTel {
  width: 191rpx;
  height: 69rpx;
  line-height: 69rpx;
  border: 2rpx solid #2576F8;
  border-radius: 48rpx;
  color: #2576F8;
  text-align: center;
  font-size: 28rpx;
  margin-left: 65rpx;
}

#index .shouhuo .o_stateTwo .callTel .telicon {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  width: 28rpx;
  height: 29rpx;
  display: inline-block;
  vertical-align: -2rpx;
}

#index .shouhuo .later {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #171A25;
  text-align: center;
  margin-top: 28rpx;
}

#index .shouhuo .laterbutton {
  background-image: linear-gradient(-90deg, #2160F4 0%, #298CFC 100%);
  border-radius: 41rpx;
  width: 595rpx;
  height: 82rpx;
  line-height: 82rpx;
  text-align: center;
  margin: 60rpx auto 0;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #FFFFFF;
  letter-spacing: 1px;
}

#index .shouhuo .gotoCent {
  width: 127rpx;
  height: 100rpx;
  border-radius: 50% 0 0 50%;
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -50rpx;
  background: #ECF2F7;
}

#index .shouhuo .gotoCent .runond {
  width: 82rpx;
  height: 82rpx;
  border-radius: 50%;
  background: #fff;
  margin: 8rpx 0 0 6px;
}

#index .shouhuo .gotoCent .runond image {
  width: 34rpx;
  height: 38rpx;
  vertical-align: middle;
  margin: 0 auto;
  display: block;
  padding-top: 18rpx;
}

#index .reasonList {
  background: #FFFFFF;
  border-radius: 8rpx;
  width: 600rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 2;
  transform: translate(-50%, -50%);
}

#index .reasonList .reasonListbox .title {
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #171A25;
  text-align: center;
  padding: 30rpx 0;
}

#index .reasonList .reasonListbox .padddiv {
  padding: 0 50rpx;
}

#index .reasonList .reasonListbox .padddiv .radio-group .radio {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #171A25;
  letter-spacing: 1rpx;
  display: block;
}

#index .reasonList .reasonListbox .padddiv .radio-group .radio radio {
  transform: scale(0.7);
}

#index .reasonList .reasonListbox .padddiv .ohter {
  width: 480rpx;
  height: 70rpx;
  line-height: 70rpx;
  border: 2rpx solid #D7D8DF;
  font-size: 28rpx;
  margin-top: 35rpx;
  padding-left: 20rpx;
}

#index .reasonList .reasonListbox .reasonbutton {
  display: flex;
  background: #fff;
  text-align: center;
  margin-top: 60rpx;
  border-radius: 8rpx;
}

#index .reasonList .reasonListbox .reasonbutton .caler {
  flex: 1;
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #858EA0;
  line-height: 84rpx;
  border-right: 2rpx solid #E3E4EA;
  border-top: 2rpx solid #E3E4EA;
}

#index .reasonList .reasonListbox .reasonbutton .sure {
  flex: 1;
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #2576F8;
  line-height: 84rpx;
  border-top: 2rpx solid #E3E4EA;
}

#index .indexbj {
