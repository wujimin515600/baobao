#receivingAddress .receivingAddress {
  padding: 0 30rpx;
}

#receivingAddress .receivingAddress .inputText {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  line-height: 105rpx;
  height: 105rpx;
  border-bottom: 2rpx solid #E3E4EA;
}

#receivingAddress .receivingAddress .inputText .inputLabel {
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #171A25;
  vertical-align: 15rpx;
  width: 284rpx;
}

#receivingAddress .receivingAddress .inputText .clickmap {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  width: 386rpx;
}

#receivingAddress .receivingAddress .inputText .clickmap .select .mapiocn {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  width: 21rpx;
  height: 27rpx;
  display: inline-block;
}

#receivingAddress .receivingAddress .inputText .clickmap .select .text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #858EA0;
  margin-left: 11rpx;
}

#receivingAddress .receivingAddress .inputText .clickmap .righticon {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  width: 12rpx;
  height: 22rpx;
  display: block;
  margin-top: 42rpx;
}

#receivingAddress .receivingAddress .inputText input {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #171A25;
  display: inline-block;
  line-height: 105rpx;
  height: 105rpx;
}

#receivingAddress .receivingAddress .inputText .phcolor {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #858EA0;
}

#receivingAddress .holdAddress {
  background: #C9CDD7;
  box-shadow: 0 15rpx 28rpx 0 rgba(201, 205, 215, 0.74);
  border-radius: 48rpx;
  width: 595rpx;
  height: 96rpx;
  line-height: 96rpx;
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #858EA0;
  text-align: center;
  margin: 83rpx auto 0;
}

#receivingAddress .selectaddress {
  background-image: linear-gradient(-90deg, #215FF4 0%, #298CFC 100%);
  box-shadow: 0 15rpx 28rpx 0 rgba(40, 132, 251, 0.57);
  border-radius: 48rpx;
  color: #fff;
}
