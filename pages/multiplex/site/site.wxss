.nodatas {
  text-align: center;
  font-size: 28rpx;
  margin-top: 40rpx;
}

.listChild {
  background: #FFFFFF;
  box-shadow: 0 3rpx 23rpx 1rpx rgba(77, 77, 77, 0.1);
  border-radius: 8rpx;
  padding: 30rpx;
  margin-top: 20rpx;
}

.listChild .listChild-title {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
}

.listChild .listChild-title .firstbox {
  display: flex;
  align-items: center;
}

.listChild .listChild-title .firstbox .address {
  display: flex;
  align-items: center;
}

.listChild .listChild-title .firstbox .address text {
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  color: #171A25;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 230rpx;
  display: inline-block;
  vertical-align: -12rpx;
  font-weight: bold;
}

.listChild .listChild-title .firstbox .address .mapicon {
  width: 24rpx;
  height: 30rpx;
  margin-right: 16rpx;
}

.listChild .listChild-title .firstbox .peoplename {
  display: flex;
  align-items: center;
}

.listChild .listChild-title .firstbox .peoplename text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #858EA0;
  width: 100rpx;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.listChild .listChild-title .firstbox .peoplename .peopleicon {
  width: 25rpx;
  height: 27rpx;
  margin-right: 12rpx;
}

.listChild .listChild-title .kmbox text {
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  color: #171A25;
}

.listChild .listChild-flex {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
  margin-top: 6rpx;
  position: relative;
}

.listChild .listChild-flex .childlabel {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  width: 88%;
}

.listChild .listChild-flex .childlabel .childlabel-list {
  display: flex;
  flex-flow: row nowrap;
  margin: 22rpx 0 0 33rpx;
  flex: auto;
  width: 47%;
}

.listChild .listChild-flex .childlabel .childlabel-list image {
  width: 81rpx;
  height: 81rpx;
  margin-right: 12rpx;
  min-width: 81rpx;
}

.listChild .listChild-flex .childlabel .childlabel-list .text .text-name {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #3A4159;
  display: block;
  line-height: 24rpx;
  margin-top: 8rpx;
}

.listChild .listChild-flex .childlabel .childlabel-list .text .text-weight {
  display: block;
  line-height: 24rpx;
  margin-top: 10rpx;
}

.listChild .listChild-flex .childlabel .childlabel-list .text .text-weight .num {
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: #171A25;
}

.listChild .listChild-flex .childlabel .childlabel-list .text .text-weight .num .kg {
  font-family: PingFangSC-Medium;
  font-size: 22rpx;
  color: #0B0013;
}

.listChild .listChild-flex .childlabel .childlabel-list:nth-child(2n+1) {
  margin-left: 0;
}

.listChild .listChild-flex .childmore {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
}

.listChild .listChild-flex .childmore text {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #858EA0;
}

.listChild .listChild-flex .childmore .moreicon {
  width: 12rpx;
  height: 23rpx;
}

.seekFailed .seek {
  width: 146rpx;
  height: 207rpx;
  display: block;
  margin: 40rpx auto 0;
}

.seekFailed .seek-title {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #858EA0;
  text-align: center;
  margin-top: 17rpx;
}

.seekFailed .twoButton {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  margin: 50rpx 50rpx 0;
}

.seekFailed .twoButton view {
  width: 241rpx;
  height: 69rpx;
  font-size: 32rpx;
  border-radius: 48rpx;
  font-family: PingFangSC-Regular;
  text-align: center;
  line-height: 69rpx;
}

.seekFailed .twoButton .refresh {
  border: 2rpx solid #2576F8;
  color: #2576F8;
  margin: 0 auto;
}

.seekFailed .twoButton .refresh .refreshicon {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  width: 25rpx;
  height: 25rpx;
  display: inline-block;
  vertical-align: -3rpx;
  margin-right: 14rpx;
}

.seekFailed .twoButton .phone {
  background: linear-gradient(-90deg, #215FF4 0%, #298CFC 100%);
  color: #FFFFFF;
  margin-left: 65rpx;
}

.seekFailed .twoButton .phone .phoneicon {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  width: 25rpx;
  height: 25rpx;
  display: inline-block;
  vertical-align: -3rpx;
  margin-right: 14rpx;
}
